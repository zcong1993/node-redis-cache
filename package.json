    "@typescript-eslint/parser": "^5.28.0",
    "@zcong/js-release": "^0.2.5",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "ioredis": "^5.0.6",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "prom-client": "^14.0.1",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
